---
layout: layouts/base.njk
bodyClass: body-index
---

<h1 id="title">{{ site.title }}</h1>
<p id="description">
    {{ site.description }}
</p>

{# Links #}
<div id="links">
    <a href="{{ site.hash_cracker.url }}" target="_blank" rel="noopener" class="hash-cracker" title="{{ site.hash_cracker.name }}">
        <img src="{{ site.hash_cracker.icon }}" alt="{{ site.hash_cracker.name }}">
    </a>
    <a href="{{ site.github.url }}" target="_blank" rel="noopener" title="{{ site.github.name }}">
        <img src="{{ site.github.icon }}" alt="{{ site.github.name }}">
    </a>
</div>
{# /Links #}

