<div id="exploits">

    {# Reconnaissance #}
    <div class="block red reconnaissance">
        <h3 class="category">Reconnaissance</h3>

        <ul class="list">
            {%- for page in search.pages("category1=reconnaissance", "title") %}
            <li>
                <a href="{{ page.data.url }}">{{ page.data.title }}</a>
            </li>
            {%- endfor %}
        </ul>
    </div>
    {# /Reconnaissance #}

